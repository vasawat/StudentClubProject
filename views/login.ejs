<%- include('header') %>
<center>
<div class="formlogin" >
<form action="/user/login" method="POST">
  <div class="mb-3">
    <center><img class="LoginRegisimg" src="/images/key.png" alt="" width="70px"></center>
  </div>
  <div class="mb-3">
    <label  class="form-label text-dark">Email address</label>
    <input type="email" class="form-control" name="username"  aria-describedby="emailHelp">
  </div>
  <div class="mb-3">
    <label  class="form-label text-dark">Password</label>
    <input type="password" name="password" class="form-control" >
  </div>
  <center><button type="submit" class="btn btn-primary LoginRegisbtn">Login</button></center>
</form>
</div>
</center>
<%- include('footer') %>